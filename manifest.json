{
  "manifest_version": 3,
  "name": "My Temp",
  "description": "Current Weather Data",
  "version": "1.0",
  "permissions": [
    "offscreen",
    "geolocation",
    "webRequest",
    "webRequestAuthProvider",
    "storage"
  ],
  "host_permissions": [ "http://api.weatherapi.com/v1" ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "main128.png"
  }
}

